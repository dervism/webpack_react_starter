{
  "name": "webpack_react_starter",
  "version": "1.0.0",
  "description": "Webpack and React Starter project",
  "main": "./app/scripts/app.js",
  "private": true,
  "scripts": {
    "clean": "rimraf dist",
    "lint": "eslint app --ext .js --ext .jsx",
    "deploy": "node ./lib/deploy.js",
    "stats": "webpack --profile --json > stats.json",
    "build": "npm run clean && webpack",
    "start": "webpack-dev-server",
    "dev": "npm start",
    "dist": "http-server -p 3000 -a 0.0.0.0 -d false dist --cors"
  },
  "author": "Dervis M",
  "license": "ISC",
  "devDependencies": {
    "babel-core": "6.18.2",
    "babel-eslint": "7.1.1",
    "babel-loader": "6.2.8",
    "babel-preset-es2015": "6.18.0",
    "babel-preset-react": "6.16.0",
    "babel-preset-stage-0": "6.16.0",
    "css-loader": "0.23.1",
    "eslint": "3.10.2",
    "eslint-loader": "1.6.1",
    "eslint-plugin-react": "6.7.1",
    "extract-text-webpack-plugin": "1.0.1",
    "file-loader": "0.8.5",
    "gh-pages": "0.12.0",
    "history": "3.2.1",
    "html-webpack-plugin": "2.24.1",
    "http-server": "x0.9.0",
    "lodash": "4.17.2",
    "node-sass": "3.13.0",
    "open-browser-webpack-plugin": "0.0.3",
    "react-hot-loader": "1.3.0",
    "rimraf": "2.5.4",
    "sass-loader": "4.0.2",
    "style-loader": "0.13.1",
    "url-loader": "0.5.7",
    "webpack": "1.13.3",
    "webpack-dev-server": "1.16.2",
    "webpack-merge": "0.14.1"
  },
  "dependencies": {
    "react": "15.4.1",
    "react-dom": "15.4.1",
    "react-redux": "4.4.5",
    "react-router": "2.8.1",
    "react-router-redux": "4.0.6",
    "react-router-scroll": "0.4.1",
    "redux": "3.5.2",
    "redux-saga": "0.10.5"
  }
